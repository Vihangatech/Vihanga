{
  "name": "Abu MD",
  "description": "Abu MD - Simple WhatsApp Multi Device Bot",
  "keywords": ["whatsapp-bot", "multi-device", "Jsl", "abu-md", "abu"],
  "repository": "https://github.com/5hefin/Alexa-MD",
  "stack": "container",
  "env": {
    "SESSION_ID": {
      "description": "session id",
      "required": true,
      "value": ""
    },
    "SUDO": {
      "description": "admin",
      "required": true,
      "value": "null"
    },
    "MODE": {
      "description": "mode",
      "required": true,
      "value": "private"
    },
    "HEROKU_APP_NAME": {
      "description": "heroku app name, same as above entered",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "heroku account api key, https://dashboard.heroku.com/account",
      "required": true
    },
    "LOG_MSG": {
      "description": "show whatsapp msgs in log",
      "required": false,
      "value": "false"
    },
    "PREFIX": {
      "description": "prefix for bot. all so can be null or false multi prefix",
      "required": true,
      "value": "."
    },
    "STICKER_PACKNAME": {
      "description": "sticker pack info",
      "required": false,
      "value": "Abu;Jsl"
    },
    "LANGUAGE": {
      "description": "bot language.",
      "required": false,
      "value": "english"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ]
}
